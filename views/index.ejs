        <div class="col-md-8">
          <a href="#login" ng-if="!userLoggedin" ng-click="scrollTo('login')">
            <div class="btn dbtn" id="loginbutton"><i class="fa fa-sign-in" aria-hidden="true"></i> Login</div>
          </a>
          <a href="#/register" ng-if="!userLoggedin">
            <div class="btn  dbtn"><i class="fa fa-user-plus" aria-hidden="true"></i> New Registration</div>
          </a>
