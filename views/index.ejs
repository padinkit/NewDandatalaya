            <button class="btn btn-default dropdown-patient" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
	                    {{userId}}
	                    <span class="caret"></span>
	                  </button>
            <ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
              <li ng-if="userType == 'patient'"><a href="#/dashboard">Dashboard</a></li>
              <li ng-if="userType == 'doctor'"><a href="#/doctor/dashboard">Dashboard</a></li>
              <li><a href="#/{{userType}}profile">Profile</a></li>
              <li ng-if="userType == 'doctor'"><a href="#/chargeSheet">Charge Sheet</a></li>
              <li><a href="#/changepassword">Change Password</a></li>
              <li role="separator" class="divider"></li>
              <li><a href="javascript:void(0)" ng-click="logout()">Logout</a></li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </section>

